# Ignore node modules
/node_modules

# Ignore compiled asset files
/public/hot
/public/storage
/public/storage/framework
/public/storage/logs

# Ignore vendor dependencies
/vendor

# Ignore environment files
.env
.env.*  # Matches .env.backup, .env.testing, etc.

# Ignore Laravel logs and cache
/storage/*.key
/storage/debugbar
/storage/oauth-private.key
/storage/oauth-public.key
/storage/logs/*
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*

# Ignore PHPUnit result cache
.phpunit.result.cache

# Ignore Laravel Homestead files
Homestead.json
Homestead.yaml
Vagrantfile

# Ignore editor-specific files (e.g., VSCode, PHPStorm, Sublime)
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore Composer lock file (if preferred)
/composer.lock

# Ignore npm/yarn logs
npm-debug.log
yarn-error.log
pnpm-debug.log
